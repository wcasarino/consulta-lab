<div class="row">
    <div class="col-md-8">
        <div class="card">
            <div class="card header">
                <h3>Ingresar Paciente a Buscar</h3>
            </div>
            <div class="card-body">
                <form action="/paciente/seek" method="POST">
                    <div class="form-group">
                        <input type="text" name="DNI" class="form-control" placeholder="DNI" autofocus>
                    </div>
                    <div class="form-group">
                        <button class="btn btn-primary btn-block" type="submit">
                            Buscar
                        </button>
                    </div>
                </form>                
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12 p-4 table-responsive" >
        {{#if pacientes}}
            <table class="table table-striped table-hover table-bordered table-sm bg-white table-responsive" 
            > 
                <thead>
                <tr>
                    <td>Fecha</td>
                    <td>Protocolo</td>
                    <td>DNI</td>
                    <td>Nombre</td>
                    <th><div class="size">Hemograma</div></th>
                    <th>Formula Leucocitaria</th>
                    <th>Orina</th>
                    <th>Examen Microscopico de Sedimento</th>
                    <th>Sedimento urinario</th>
                    <th>Antibiograma</th>
                    <th>Analisis quimico</th>
                    <th>Enzimas</th>
                    <th>Bilirrubina</th>
                    <th>Coagulacion</th>
                    <th>PRUEBA DE TOLERANCIA ORAL A LA GLUCOSA</th>
                    <th>Grupo Sanguineo</th>
                    <th>Factor RH</th>
                    <th>Serologia</th>
                    <th>HCG EN SANGRE</th>
                    <th>PSA (prostatico)</th>
                    <th>POLIMORFOS NUCLEARES EN MATERIA FECAL</th>
                    <th>COPROPARASITOLOGICO SERIADO</th>
                    <th>ESCOBILLADO ANAL</th>
                    <th>PROTEINURIA (24 hs)</th>
                    <th>EXUDADO VAGINAL - ANAL</th>
                    <th>HbA1c (hemoglobina glicosilada)</th>
                    <th>TIROIDE (TSH)</th>
                    <th>TIROIDE (T4)</th>
                    <th>STROUT</th>
                    <th>IGA -TGG (celiuaquia)</th>
                    <th>SOMF (sangre oculta en materia fecal)</th>
                    <th>IONOGRAMA</th>

                    {{!-- <td>Acciones</td> --}}

                </tr>
                </thead>
                <tbody >
                    {{#each pacientes}}
                    <tr>
                        {{!-- <td>{{formatDate Fecha day="numeric" month="long" year="numeric"}}</td> --}}
                        <td>{{formatFecha Fecha}}</td>
                        <td>{{Protocolo}}</td>
                        <td>{{Paciente.DNI}}</td>
                        <td>{{Paciente.Nombre}}</td>
                        <th>{{Hemograma}}</th>
                        <th style="width:500px">{{'Formula Leucocitaria'}}</th>
                        <th>{{Orina}}</th>
                        <th>{{'Examen Microscopico de Sedimento'}}</th>
                        <th>{{'Sedimento urinario'}}</th>
                        <th>{{Antibiograma}}</th>
                        <th>{{'Analisis quimico'}}</th>
                        <th>{{Enzimas}}</th>
                        <th>{{Bilirrubina}}</th>
                        <th>{{Coagulacion}}</th>
                        <th>{{'PRUEBA DE TOLERANCIA ORAL A LA GLUCOSA'}}</th>
                        <th>{{'Grupo Sanguineo'}}</th>
                        <th>{{'Factor RH'}}</th>
                        <th>{{Serologia}}</th>
                        <th>{{'HCG EN SANGRE'}}</th>
                        <th>{{'PSA (prostatico)'}}</th>
                        <th>{{'POLIMORFOS NUCLEARES EN MATERIA FECAL'}}</th>
                        <th>{{'COPROPARASITOLOGICO SERIADO'}}</th>
                        <th>{{'ESCOBILLADO ANAL'}}</th>
                        <th>{{'PROTEINURIA (24 hs)'}}</th>
                        <th>{{'EXUDADO VAGINAL - ANAL'}}</th>
                        <th>{{'HbA1c (hemoglobina glicosilada)'}}</th>
                        <th>{{'TIROIDE (TSH)'}}</th>
                        <th>{{'TIROIDE (T4)'}}</th>
                        <th>{{STROUT}}</th>
                        <th>{{'IGA -TGG (celiuaquia)'}}</th>
                        <th>{{'SOMF (sangre oculta en materia fecal)'}}</th>
                        <th>{{IONOGRAMA}}</th>

                        {{!-- <td>
                            <form action="/protocolo/seek/{{_id}}" method="POST">
                                <div class="form-group">
                                    <button class="btn btn-primary btn-block" type="submit">
                                        Ver
                                    </button>
                                </div>
                            </form>
                        </td> --}}
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        {{else}}
            <div class="card mx-auto">
                <div class="card-body">
                    <h3 class="px-auto" >SIN PACIENTE</h3>
                </div>
            </div>
        {{/if}}
    </div>
</div>

